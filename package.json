{
	"name": "ff0000-ad-tech-compiler",
	"version": "1.0.0",
	"description": "FF0000 Ad Tech - Compiler: Provides backend modules for running Creative Server, which compiles the various Builds.",
	"main": "webpack.config.js",
	"scripts": {
		"postinstall": "npm run install-build && npm run eslint",
		"install-build": "cd ./1-build && npm install && cd ../",
		"reinstall": "rm -rf ./node_modules && rm ./package-lock.json && npm i",
		"eslint": "./node_modules/.bin/eslint ./1-build --ignore-pattern 1-build/node_modules --fix",
		"start": "npm run browser",
		"server": "node ./node_modules/@ff0000-ad-tech/wp-creative-server/index.js --context ./",
		"browser": "node ./node_modules/@ff0000-ad-tech/wp-creative-server/index.js --context ./ --browser"
	},
	"repository": "https://greg.connell@stash.ff0000.com/scm/at/ad-es6.git",
	"author": "RED Interactive Agency",
	"license": "MIT",
	"devDependencies": {
		"@ff0000-ad-tech/debug": "latest",
		"@ff0000-ad-tech/fba-loader": "latest",
		"@ff0000-ad-tech/webpack-rollup-babel-loader": "latest",
		"@ff0000-ad-tech/wp-creative-server": "latest",
		"@ff0000-ad-tech/wp-deploy-manager": "latest",
		"@ff0000-ad-tech/wp-process-manager": "latest",
		"babel-plugin-dynamic-import-webpack": "^1.0.2",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.1",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"eslint": "^4.18.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-prettier": "^2.5.0",
		"lodash": "^4.17.4",
		"prettier": "^1.10.2",
		"promise-polyfill": "^7.0.0",
		"url-loader": "^0.6.2",
		"webpack": "^3.8.1",
		"webpack-cli": "^2.0.9"
	},
	"dependencies": {
		"@ff0000-ad-tech/cs-plugin-bulk-compile": "latest",
		"@ff0000-ad-tech/cs-plugin-vendor-indexes": "latest"
	}
}